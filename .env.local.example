# Base configuration shared by both providers
# Set the provider to either "nanobanana" (default) or "replicate"
NANO_BANANA_PROVIDER=nanobanana

# API target and credentials
# When using the official NanoBanana API these default values are used if left empty:
#   NANO_BANANA_BASE_URL=https://api.nanobanana.dev
#   NANO_BANANA_API_KEY=<your-nanobanana-key>
# When using Replicate-hosted NanoBanana models set:
#   NANO_BANANA_PROVIDER=replicate
#   NANO_BANANA_BASE_URL=https://api.replicate.com/v1/predictions
#   NANO_BANANA_API_KEY=<your-replicate-token>
NANO_BANANA_BASE_URL=
NANO_BANANA_API_KEY=

# Replicate-specific configuration (only needed when provider=replicate)
# Provide either NANO_BANANA_REPLICATE_MODEL or NANO_BANANA_REPLICATE_VERSION
NANO_BANANA_REPLICATE_MODEL=
NANO_BANANA_REPLICATE_VERSION=

# Optional: configure remote image hosts for Next/Image (comma separated hostnames)
# Examples:
#   NEXT_IMAGE_REMOTE_PATTERNS=replicate.delivery
#   NEXT_IMAGE_REMOTE_PATTERNS=replicate.delivery,cdn.nanobanana.dev
NEXT_IMAGE_REMOTE_PATTERNS=
