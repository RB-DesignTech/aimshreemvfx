# Base configuration shared by both providers
NANO_BANANA_BASE_URL=
NANO_BANANA_API_KEY=

# Provider can be "nanobanana" (default) or "replicate"
NANO_BANANA_PROVIDER=nanobanana

# Optional: configure remote image hosts for Next/Image (comma separated hostnames)
NEXT_IMAGE_REMOTE_PATTERNS=

# Replicate-specific configuration (only needed when provider=replicate)
# Provide either NANO_BANANA_REPLICATE_MODEL or NANO_BANANA_REPLICATE_VERSION
NANO_BANANA_REPLICATE_MODEL=
NANO_BANANA_REPLICATE_VERSION=
